{
	"name": "youtube-mate",
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "npm-run-all --parallel vite:dev typesafe-i18n",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"format": "prettier --write .",
		"lint": "prettier --check . && eslint .",
		"test:unit": "vitest",
		"test": "npm run test:unit -- --run && npm run test:e2e",
		"test:e2e": "playwright test",
		"typesafe-i18n": "typesafe-i18n",
		"vite:dev": "vite dev"
	},
	"devDependencies": {
		"@playwright/test": "^1.45.3",
		"@skeletonlabs/skeleton": "^2.10.3",
		"@skeletonlabs/tw-plugin": "^0.4.0",
		"@sveltejs/adapter-auto": "^3.0.0",
		"@sveltejs/kit": "^2.0.0",
		"@sveltejs/vite-plugin-svelte": "^4.0.0",
		"@tabler/icons-svelte": "^3.22.0",
		"@tailwindcss/container-queries": "^0.1.1",
		"@tailwindcss/forms": "^0.5.9",
		"@tailwindcss/typography": "^0.5.15",
		"@types/eslint": "^9.6.0",
		"autoprefixer": "^10.4.20",
		"eslint": "^9.7.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.36.0",
		"globals": "^15.0.0",
		"npm-run-all": "^4.1.5",
		"postcss": "^8.4.49",
		"postcss-load-config": "^6.0.1",
		"prettier": "^3.3.2",
		"prettier-plugin-svelte": "^3.2.6",
		"prettier-plugin-tailwindcss": "^0.6.5",
		"prisma": "^5.22.0",
		"svelte": "^5.0.0",
		"svelte-check": "^4.0.0",
		"svelte-preprocess": "^6.0.3",
		"tailwindcss": "^3.4.9",
		"typesafe-i18n": "^5.26.2",
		"typescript": "^5.0.0",
		"typescript-eslint": "^8.0.0",
		"vite": "^5.0.3",
		"vitest": "^2.0.4"
	},
	"dependencies": {
		"@prisma/client": "^5.22.0"
	},
	"license": "MIT",
	"engines": {
		"npm": ">= 7.0.0",
		"yarn": "Please use npm instead of yarn.",
		"pnpm": "Please use npm instead of pnpm."
	},
	"bugs": {
		"url": "https://github.com/SedatUygur/YouTube-Mate/issues"
	},
	"keywords": [
		"YouTube",
		"YouTubeMate",
		"YouTube Mate",
		"YouTube-Mate",
		"svelte",
		"typescript",
		"tailwindcss"
	],
	"author": "Sedat Uygur ('https://github.com/SedatUygur')",
	"repository": {
		"type": "git",
		"url": "git@github.com:SedatUygur/YouTube-Mate.git"
	}
}
